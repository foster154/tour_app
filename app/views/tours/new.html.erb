<% provide(:title, 'Create Tour') %>
<h1>Create a Tour</h1>

<div class="row">
	<div class="col-md-4 col-md-offset-4">
		<%= form_for(@tour, user_id: @user.id) do |f| %>
			<%= render 'shared/tour_error_messages' %>

			<%= render(:partial => "form", :locals => {:f => f}) %>

			<%= f.submit "Create Tour", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>
