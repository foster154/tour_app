<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>

  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>

  <script>
    jQuery(function() {

      jQuery('#bannerscollection_zoominout_generous').bannerscollection_zoominout({
        skin: 'generous',
        responsive:true,
        width: 900,
        height: 650,
        width100Proc:false,
        numberOfThumbsPerScreen:7,
        thumbsOnMarginTop:11,
        thumbsWrapperMarginTop: -110,
        autoHideBottomNav:true,
        autoPlay: 4,
        showOnInitBottomNav: false,
        showOnInitNavArrows: false
      });   
      
      
    });
  </script>
  

  <!-- Map -->
  <script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
  <script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>

  <!-- Fonts -->
  <link href='https://fonts.googleapis.com/css?family=Cabin:700,400' rel='stylesheet' type='text/css'>

  <!-- Other -->
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>

<body>
  <div class="container-fluid">
    <div class="row">
      <%= render 'layouts/tour-elegant/header' %>
    </div>

    <div class="row gallery-pad">
      <%= render 'layouts/tour-elegant/gallery' %>
    </div>

    <a name="info" id="info"><hr class="margin-top" /></a>
    <div class="row">
      <% if @branded == true %>
        <%= render 'layouts/tour-elegant/agent_and_share' %>
      <% else %>
        <%= render 'layouts/tour-elegant/share' %>
      <% end %>
    </div>

    <hr class="margin-top" />

    <div class="row">
      <%= render 'layouts/tour-elegant/info_panels' %>
    </div>

    <div class="row">
      <%= render 'layouts/tour-elegant/footer' %>
    </div>
  </div>

		<%= yield %>
		<% # debug(params) if Rails.env.development? %>
</body>
</html>